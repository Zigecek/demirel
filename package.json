{
  "name": "demirel",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "nodemon -w src/server -x tsx src/server/index.ts",
    "migrate:dev": "prisma migrate dev",
    "migrate:deploy": "prisma migrate deploy",
    "start:tsx": "cross-env NODE_ENV=production tsx src/server/index.ts",
    "build:react": "vite build --config vite.client.config.ts",
    "build:server": "prisma generate && tsup",
    "build": "npm run build:react && npm run build:server",
    "start:node": "node ./dist/index.js",
    "start:pm2": "pm2 delete demirel && pm2 start ecosystem.config.cjs && pm2 save",
    "update": "npx npm-check-updates -u -t semver",
    "pull": "git reset --hard && git pull",
    "workflow": "npm run pull && npm i && npm run migrate:deploy && npm run build && npm run start:pm2 && pm2 list"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/material": "^6.3.1",
    "@mui/styled-engine": "^6.3.1",
    "@prisma/client": "^6.1.0",
    "argon2": "^0.41.1",
    "axios": "^1.7.9",
    "chalk": "^5.4.1",
    "chart.js": "^4.4.7",
    "chartjs-adapter-date-fns": "^3.0.0",
    "chartjs-plugin-annotation": "^3.1.0",
    "chartjs-plugin-zoom": "^2.2.0",
    "connect-pg-simple": "^10.0.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.7",
    "envalid": "^8.0.0",
    "eventemitter3": "^5.0.1",
    "express": "^5.0.1",
    "express-session": "^1.18.1",
    "http-status-codes": "^2.3.0",
    "lodash": "^4.17.21",
    "mqtt": "^5.10.3",
    "pg": "^8.13.1",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.4.0",
    "react-router-dom": "^7.1.1",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "tailwind-scrollbar": "^3.1.0",
    "vite-express": "*",
    "web-push": "^3.6.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.3.1",
    "@rollup/plugin-typescript": "^12.1.2",
    "@types/chartjs-plugin-crosshair": "^1.1.4",
    "@types/connect-pg-simple": "^7.0.3",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/express-session": "^1.18.1",
    "@types/lodash": "^4.17.14",
    "@types/node": "^22.10.5",
    "@types/pg": "^8.11.10",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/web-push": "^3.6.4",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.17.0",
    "globals": "^15.14.0",
    "nodemon": "^3.1.9",
